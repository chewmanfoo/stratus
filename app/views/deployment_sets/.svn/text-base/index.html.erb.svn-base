<%- model_class = DeploymentSet -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.pluralize %></h1>
</div>
<table class="table table-striped">
  <thead>
    <tr>
      <th><%= model_class.human_attribute_name(:id) %></th>
      <th><%= model_class.human_attribute_name(:application_id) %></th>
      <th><%= model_class.human_attribute_name(:build_number) %></th>
      <th><%= model_class.human_attribute_name(:run_pre_checkout) %></th>
      <th><%= model_class.human_attribute_name(:run_post_checkout) %></th>
      <th><%= model_class.human_attribute_name(:changelist) %></th>
      <th><%= model_class.human_attribute_name(:auto_accept) %></th>
      <th><%= model_class.human_attribute_name(:auto_start) %></th>
      <th><%= model_class.human_attribute_name(:created_by) %></th>
      <th><%= model_class.human_attribute_name(:created_at) %></th>
      <th><%=t '.actions', :default => t("helpers.actions") %></th>
    </tr>
  </thead>
  <tbody>
    <% @deployment_sets.each do |deployment_set| %>
      <tr>
        <td><%= link_to deployment_set.id, deployment_set_path(deployment_set) %></td>
        <td><%= deployment_set.application_id %></td>
        <td><%= deployment_set.build_number %></td>
        <td><%= deployment_set.run_pre_checkout %></td>
        <td><%= deployment_set.run_post_checkout %></td>
        <td><%= deployment_set.changelist %></td>
        <td><%= deployment_set.auto_accept %></td>
        <td><%= deployment_set.auto_start %></td>
        <td><%= deployment_set.created_by %></td>
        <td><%=l deployment_set.created_at %></td>
        <td>
          <%= link_to t('.edit', :default => t("helpers.links.edit")),
                      edit_deployment_set_path(deployment_set), :class => 'btn btn-mini' %>
          <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
                      deployment_set_path(deployment_set),
                      :method => :delete,
                      :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
                      :class => 'btn btn-mini btn-danger' %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>

<%= link_to t('.new', :default => t("helpers.links.new")),
            new_deployment_set_path,
            :class => 'btn btn-primary' %>
